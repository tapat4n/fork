{
  "name": "tapat4n/fork",
  "description": "Fork manager for php",
  "keywords": [
    "fork",
    "library",
    "process",
    "thread",
    "php",
    "pcntl",
    "shmop"
  ],
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Tapat4n\\Fork\\": "src/"
    }
  },
  "authors": [
    {
      "name": "tapat4n",
      "email": "volansky.sasha@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=8.1"
  },
  "suggest": {
    "ext-shmop": "Required for shmop handler support",
    "ext-pcntl": "Required for pcntl support"
  },
  "require-dev": {
    "vimeo/psalm": "*",
    "symfony/var-dumper": "*",
    "phpunit/phpunit": "*"
  }
}
